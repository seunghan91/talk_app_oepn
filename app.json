{
  "expo": {
    "name": "TALKK",
    "slug": "talk-app",
    "version": "1.0.0",
    "scheme": "talkk",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "음성 메시지를 통해 새로운 인연을 만나는 소셜 네트워킹 앱",
    "runtimeVersion": "appVersion",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talkapp.talkk2025",
      "buildNumber": "4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.talkapp.talkk2025"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "apiUrl": "https://talkk-api.onrender.com",
      "eas": {
        "projectId": "a6b66f7d-73bf-4df8-9cc4-c78af6984b8b"
      }
    },
    "owner": "seunghan91"
  }
}
