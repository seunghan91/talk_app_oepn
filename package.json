{
  "name": "talkk-app",
  "main": "expo-router/entry.js",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "start-tunnel": "expo start --tunnel",
    "tunnel": "expo start --tunnel --port 8083",
    "tunnel-no-dev": "expo start --tunnel --port 8083 --no-dev",
    "fix-deps": "node ./scripts/fix-podfile-dependencies.js",
    "clean-ios": "cd ios && rm -rf Pods Podfile.lock && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "rebuild-ios": "npm run clean-ios && npm run fix-deps",
    "build:increment": "node ./scripts/increment-build-number.js",
    "build:set": "node ./scripts/increment-build-number.js --set",
    "build:ios": "npm run build:increment && eas build -p ios --profile production --non-interactive",
    "build:android": "npm run build:increment && eas build -p android --profile production --non-interactive",
    "build:all": "npm run build:increment && eas build --platform all --profile production --non-interactive",
    "submit:ios": "eas submit -p ios --profile production --non-interactive",
    "submit:android": "eas submit -p android --profile production --non-interactive",
    "generate:types": "node ./scripts/generate-api-types.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/server": "^0.5.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.5",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.21",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "i18next": "^24.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.4.1",
    "react-native": "^0.76.6",
    "react-native-crypto": "^2.2.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-iap": "^12.16.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-redux": "^9.2.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^18.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "metro": "^0.81.0",
    "openapi-typescript": "^7.8.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@types/react": "^18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "private": true
}
