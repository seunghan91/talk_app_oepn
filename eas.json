{
  "cli": {
    "version": ">= 5.4.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "cache": {
        "disabled": true
      },
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "credentialsSource": "remote",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "credentialsSource": "remote",
        "buildConfiguration": "Release",
        "distribution": "store"
      }
    },
    "test": {
      "distribution": "internal",
      "channel": "test",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "buildConfiguration": "Release",
        "distribution": "internal"
      }
    }
  },
  "submit": {
    "development": {
      "ios": {
        "appleId": "${APPLE_ID}",
        "ascAppId": "${APPLE_ASC_APP_ID}",
        "appleTeamId": "${APPLE_TEAM_ID}"
      }
    },
    "production": {
      "ios": {
        "appleId": "theqwe2000@naver.com",
        "ascAppId": "127717087",
        "appleTeamId": "AVMJBATWAT"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "test": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
