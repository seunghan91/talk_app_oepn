{
  "cli": {
    "version": ">= 5.4.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "ios": {
        "autoIncrement": "buildNumber",
        "env": {
          "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "ios": {
        "autoIncrement": "buildNumber"
      }
    },
    "production": {
      "cache": {
        "disabled": true
      },
      "ios": {
        "autoIncrement": "buildNumber",
        "cache": {
          "disabled": true
        },
        "env": {
          "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
        }
      },
      "android": {
        "cache": {
          "disabled": true
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
} 